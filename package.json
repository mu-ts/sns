{
  "name": "@mu-ts/sns",
  "version": "2.1.0",
  "description": "Syntactical sugar for working with SNS topics in TypeScript via the AWS-SDK@3",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p .",
    "test": "mocha -r ts-node/register spec/**/*.spec.ts",
    "clean": "rm -rf coverage lib .nyc_output"
  },
  "files": [
    "dist/*",
    "README.md",
    "LICENSE",
    "CODE_OF_CONDUCT.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mu-ts/sns.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mu-ts/sns/issues"
  },
  "homepage": "https://github.com/mu-ts/sns#readme",
  "dependencies": {
    "@mu-ts/logger": "^3.2.0",
    "@mu-ts/serialization": "^1.2.0"
  },
  "peerDependencies": {
    "@aws-sdk/client-sns": "^3.431.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.9",
    "@types/mocha": "^10.0.3",
    "@types/node": "^20.8.7",
    "@types/uuid": "^9.0.6",
    "chai": "^4.3.10",
    "mocha": "^10.2.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "nyc": {
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true
  }
}
