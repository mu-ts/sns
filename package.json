{
  "name": "@mu-ts/sns",
  "version": "1.0.8",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "tslint 'src/**/*.ts'",
    "start": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "xtest": "nyc mocha test",
    "codecov": "mkdir .nyc_output && nyc report --reporter=json && codecov -f coverage/*.json"
  },
  "files": [
    "dist/*",
    "README.md",
    "LICENSE",
    "CODE_OF_CONDUCT.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mu-ts/sns.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mu-ts/sns/issues"
  },
  "homepage": "https://github.com/mu-ts/sns#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.20",
    "@types/sinon": "^7.0.13",
    "aws-sdk": "^2.430.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.2.0",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "sinon": "^7.4.1",
    "source-map-support": "^0.5.11",
    "ts-node": "^8.0.3",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "@types/aws-lambda": "^8.10.23",
    "reflect-metadata": "^0.1.13"
  },
  "nyc": {
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true
  }
}
